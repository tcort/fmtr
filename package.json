{
  "name": "fmtr",
  "version": "1.1.0",
  "description": "awesome string formatter",
  "main": "index.js",
  "scripts": {
    "pretest": "jshint index.js",
    "test": "mocha -R spec"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tcort/fmtr"
  },
  "keywords": [
    "format",
    "string",
    "placeholder",
    "substitution"
  ],
  "author": "Thomas Cort <linuxgeek@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tcort/fmtr/issues"
  },
  "homepage": "https://github.com/tcort/fmtr",
  "dependencies": {
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "jshint": "^2.9.7"
  },
  "jshintConfig": {
    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "globalstrict": true,
    "immed": true,
    "indent": 4,
    "moz": true,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "trailing": true,
    "undef": true,
    "smarttabs": true,
    "strict": true,
    "validthis": true,
    "globals": {
      "describe": false,
      "it": false,
      "before": false,
      "beforeEach": false,
      "after": false,
      "afterEach": false
    }
  }
}
